{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
       
      ]
    },
    "@my-turbo/frontend#dev": {
      "cache": false,
      "persistent": true
    },
    "@my-turbo/redis-master#dev": {
      "cache": false,
      "persistent": true
    },
    "@my-turbo/redis-worker#dev": {
      "cache": false,
      "persistent": true
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        "apps/frontend/dist/**",
        "apps/backend/redisMaster/dist/**", 
        "apps/backend/redisWorker/dist/**"
      ]
    },
    "lint": {
      "dependsOn": ["^lint"]
    }
  }
}
